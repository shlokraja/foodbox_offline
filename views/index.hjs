    <script>
        $(document).ready(function () {
            $('.signal').hide();
            $('#overlay').hide();
            // Initializing material design
            $.material.init();
            // Initializing the data bindings
            initDataBindings();
            getIssueEnum();
            if (simpleStorage.get("test_mode") == true) {
                $("#test_mode-dialog").modal({ show: true, backdrop: "static" });
            }
            var outlet_url = process.env.OUTLET_HOST;
            var run = function () {
                var req = new XMLHttpRequest();
                req.timeout = 5000;
                req.open('GET', outlet_url, true);
                req.send();
            }
            setInterval(run, 3000);
        });
        $('.dropdown').on('show.bs.dropdown', function () {
            var length = (parseInt($('.dropdown-menu').css('width'), 10) * -1) + 15;
            $('.dropdown-menu').css('left', length);
        })
    </script>
